{
  "name": "back1ng/chatgptlaravelignition",
  "description": "Adds the ChatGPT response to the error hint from laravel-ignition",
  "keywords": [
    "dev",
    "chatgpt",
    "testing"
  ],
  "license": ["Apache-2.0"],
  "authors": [
    {
      "name": "Anatoly Elyutin",
      "email": "anatolyu128@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Back1ng\\ChatGPTLaravelIgnition\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "openai-php/client": "^0.3.0",
    "spatie/laravel-ignition": "^1.0|^2.0",
    "laravel/framework": "^8.0|^9.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Back1ng\\ChatGPTLaravelIgnition\\ServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpstan/extension-installer": "^1.2",
    "phpstan/phpstan-strict-rules": "^1.4",
    "thecodingmachine/phpstan-strict-rules": "^1.0",
    "ergebnis/phpstan-rules": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
